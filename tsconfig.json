
{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "strictNullChecks": false,
    "noImplicitAny": false,  
    "baseUrl": "./src/pages",  
     
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/nhap.js"],
  "exclude": ["node_modules"], 
}
